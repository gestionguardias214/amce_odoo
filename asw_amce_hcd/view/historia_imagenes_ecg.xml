<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="view_form_hosp" model="ir.ui.view">
            <field name="name">Imagenes de Historias Clinicas</field>
            <field name="model">asw.historia_imagenes_ecg</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" invisible='1'/>
