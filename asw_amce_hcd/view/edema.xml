<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="view_form_edemas" model="ir.ui.view">
            <field name="name">Edemas</field>
            <field name="model">asw.edema</field>
            <field name="arch" type="xml">
                <form string="Edemas">
                    <sheet>
                        <group string="Descripcion" col='4'>
                            <field name="edema_nombre" />
                            <field name="active" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_tree_edemas" model="ir.ui.view">
            <field name="name">Edemas</field>
            <field name="model">asw.edema</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="edema_nombre" />
                </tree>
            </field>
        </record>

        <record id="view_search_edemas" model="ir.ui.view">
            <field name="name">Edemas</field>
            <field name="model">asw.edema</field>
            <field name="arch" type="xml">
                <search>
                    <field name="edema_nombre" />
                </search>
            </field>
        </record>

        <record id="action_edema_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Edemas</field>
            <field name="res_model">asw.edema</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="target">current</field>
        </record>
    </data>
</odoo>